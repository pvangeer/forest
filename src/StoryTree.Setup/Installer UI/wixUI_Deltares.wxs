<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs">
  <Fragment>
    <UI Id="WixUI_Deltares">
      <TextStyle Id="WixUI_Font_Normal" FaceName="Tahoma" Size="8" />
      <TextStyle Id="WixUI_Font_Bigger" FaceName="Tahoma" Size="12" />
      <TextStyle Id="WixUI_Font_Title" FaceName="Tahoma" Size="9" Bold="yes" Red="255" Green="255" Blue="255" />

      <TextStyle Id="WixUI_Font_Banner_Description" FaceName="Tahoma" Size="8" Red="255" Green="255" Blue="255" />

      <Property Id="DefaultUIFont" Value="WixUI_Font_Normal" />

      <DialogRef Id="BrowseDlgDeltares" />
      <DialogRef Id="ErrorDlgDeltares" />
      <DialogRef Id="FatalErrorDeltares" />
      <DialogRef Id="FilesInUse" />
      <DialogRef Id="FilesInUseDeltares" />
      <DialogRef Id="MsiRMFilesInUseDeltares" />
      <DialogRef Id="PrepareDlgDeltares" />
      <DialogRef Id="ProgressDlgDeltares" />
      <DialogRef Id="ResumeDlgDeltares" />
      <DialogRef Id="UserExitDeltares" />

      <Publish Dialog="ExitDialogDeltares" Control="Finish" Event="EndDialog" Value="Return" Order="999" />

      <Publish Dialog="BrowseDlgDeltares" Control="OK" Event="DoAction" Value="WixUIValidatePath_$(sys.BUILDARCHSHORT)" Order="1" />
      <Publish Dialog="BrowseDlgDeltares" Control="OK" Event="SpawnDialog" Value="InvalidDirDlgDeltares" Order="2" Condition="WIXUI_INSTALLDIR_VALID&lt;&gt;&quot;1&quot;" />

      <Publish Dialog="WelcomeDlgDeltares" Control="Next" Event="NewDialog" Value="InstallDirDlgDeltares" />

      <Publish Dialog="InstallDirDlgDeltares" Control="Back" Event="NewDialog" Value="WelcomeDlgDeltares" />
      <Publish Dialog="InstallDirDlgDeltares" Control="Next" Event="SetTargetPath" Value="[WIXUI_INSTALLDIR]" Order="1" />
      <Publish Dialog="InstallDirDlgDeltares" Control="Next" Event="DoAction" Value="WixUIValidatePath_$(sys.BUILDARCHSHORT)" Order="2" Condition="NOT WIXUI_DONTVALIDATEPATH" />
      <Publish Dialog="InstallDirDlgDeltares" Control="Next" Event="SpawnDialog" Value="InvalidDirDlgDeltares" Order="3" Condition="NOT WIXUI_DONTVALIDATEPATH AND WIXUI_INSTALLDIR_VALID&lt;&gt;&quot;1&quot;" />
      <Publish Dialog="InstallDirDlgDeltares" Control="Next" Event="NewDialog" Value="VerifyReadyDlgDeltares" Order="4" Condition="WIXUI_DONTVALIDATEPATH OR WIXUI_INSTALLDIR_VALID=&quot;1&quot;" />
      <Publish Dialog="InstallDirDlgDeltares" Control="ChangeFolder" Property="_BrowseProperty" Value="[WIXUI_INSTALLDIR]" Order="1" />
      <Publish Dialog="InstallDirDlgDeltares" Control="ChangeFolder" Event="SpawnDialog" Value="BrowseDlgDeltares" Order="2" />

      <Publish Dialog="VerifyReadyDlgDeltares" Control="Back" Event="NewDialog" Value="InstallDirDlgDeltares" />

      <Publish Dialog="MaintenanceWelcomeDlgDeltares" Control="Next" Event="NewDialog" Value="MaintenanceTypeDlgDeltares" />

      <Publish Dialog="MaintenanceTypeDlgDeltares" Control="RepairButton" Event="NewDialog" Value="VerifyReadyDlgDeltares" />
      <Publish Dialog="MaintenanceTypeDlgDeltares" Control="RemoveButton" Event="NewDialog" Value="VerifyReadyDlgDeltares" />
      <Publish Dialog="MaintenanceTypeDlgDeltares" Control="Back" Event="NewDialog" Value="MaintenanceWelcomeDlgDeltares" />
    </UI>
    
    <UIRef Id="WixUI_Common" />
  </Fragment>
</Wix>