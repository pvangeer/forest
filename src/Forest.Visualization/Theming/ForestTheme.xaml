<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:fluent="urn:fluent-ribbon"
                    mc:Ignorable="options d">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Forest.Visualization;Component/Theming/DeltaresColorTheme.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- Matadata -->
    <system:String x:Key="Theme.Name">Deltares</system:String>
    <system:String x:Key="Theme.Origin">Deltares</system:String>
    <system:String x:Key="Theme.DisplayName">Deltares</system:String>
    <system:String x:Key="Theme.BaseColorScheme">Deltares huisstijl</system:String>
    <system:String x:Key="Theme.ColorScheme">Deltares</system:String>
    <system:String x:Key="Theme.AlternativeColorScheme">Nope</system:String>

    <system:Double x:Key="DefaultItemHeight">35</system:Double>

    <Thickness x:Key="DefaultItemHeightThickness"
               Top="{StaticResource DefaultItemHeight}"
               Left="{StaticResource DefaultItemHeight}"
               Right="{StaticResource DefaultItemHeight}"
               Bottom="{StaticResource DefaultItemHeight}" />

    <DynamicResource x:Key="Theme.PrimaryAccentColor" ResourceKey="Deltares.Colors.Yellow" />
    <SolidColorBrush x:Key="Theme.ShowcaseBrush" Color="{StaticResource Deltares.Colors.Yellow}" options:Freeze="True" />
    <system:Boolean x:Key="Theme.IsHighContrast">true</system:Boolean>

    <!-- COLORS -->
    <Color x:Key="BackgroundEditableColor">AliceBlue</Color>
    <Color x:Key="BackgroundColor">White</Color>
    <Color x:Key="SelectedElementBorderColor">Red</Color>
    <Color x:Key="ApplicationBorderColor">RoyalBlue</Color>

    <SolidColorBrush Color="{StaticResource SelectedElementBorderColor}"
                     x:Key="SelectedElementBorderColorBrush" />
    <SolidColorBrush Color="{StaticResource BackgroundColor}" x:Key="BackgroundColorBrush" />
    <SolidColorBrush Color="{StaticResource BackgroundEditableColor}" x:Key="BackgroundEditableColorBrush" />
    <SolidColorBrush Color="{StaticResource ApplicationBorderColor}" x:Key="ApplicationBorderColorBrush" />

    <!-- Accent colors -->
    <Color x:Key="Fluent.Ribbon.Colors.AccentBase">#00B389</Color>
    <!-- 80% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent80">#CC00B389</Color>
    <!-- 60% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent60">#9900B389</Color>
    <!-- 40% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent40">#6600B389</Color>
    <!-- 20% -->
    <Color x:Key="Fluent.Ribbon.Colors.Accent20">#3000B389</Color>

    <Color x:Key="Fluent.Ribbon.Colors.Highlight">#00B389</Color>

    <!-- Base colors -->
    <Color x:Key="Fluent.Ribbon.Colors.Black">Black</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Black20">#30000000</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White">White</Color>
    <Color x:Key="Fluent.Ribbon.Colors.White20">#30FFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray1">#F2F2F2</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray2">#F2F2F2</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray3">#F2F2F2</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray4">#F2F2F2</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray5">#F2F2F2</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray6">#E6E6E6</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray7">#E6E6E6</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray8">#E6E6E6</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray9">#E6E6E6</Color>
    <Color x:Key="Fluent.Ribbon.Colors.Gray10">#E6E6E6</Color>

    <Color x:Key="Fluent.Ribbon.Colors.TransparentWhite">#00FFFFFF</Color>
    <Color x:Key="Fluent.Ribbon.Colors.HighTransparentWhite">#17FFFFFF</Color>

    <!-- Foreground -->
    <Color x:Key="Fluent.Ribbon.Colors.IdealForeground">Black</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkIdealForegroundDisabled">#80000000</Color>

    <!-- Misc colors -->
    <Color x:Key="Fluent.Ribbon.Colors.ExtremeHighlight">#FFD814</Color>
    <Color x:Key="Fluent.Ribbon.Colors.DarkExtremeHighlight">#FFD814</Color>

